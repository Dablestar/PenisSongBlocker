{
    "manifest_version": 3,
    "name": "PenisSongBlocker",
    "description": "Block Disgusting Songs",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "icon.png"
    },
    "permissions": ["tabs", "activeTab", "scripting", "nativeMessaging"],
    "host_permissions": [
    "https://www.youtube.com/*"
    ],
    "content_scripts": [{
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }]
  }