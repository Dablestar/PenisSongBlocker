{
  "manifest_version": 3,
  "name": "PenisSongBlocker",
  "description": "Block Disgusting Songs",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "./src/icon.png"
  },
  "permissions": ["tabs", "activeTab", "scripting", "nativeMessaging", "notifications", "storage"],
  "host_permissions": [
    "https://www.youtube.com/*"
  ],
  "background": {
    "service_worker": "./js/background.js",
    "type": "module"
  }
}